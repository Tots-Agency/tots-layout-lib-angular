    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@tots/cloud-storage": "^15.0.1",
    "@tots/core": "^15.0.13",
    "@tots/filter-box": "^15.0.4",
    "@tots/form": "^15.0.28",
    "@tots/loading": "^15.0.1",
    "@tots/table": "^15.0.27",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
