{
  "name": "tots-layout-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build2": "ng build --project=@tots/layout --configuration production && cd dist/tots/layout/ && npm publish --access public",
    "build3": "ng build --project=@tots/confirm-modal --configuration production && cd dist/tots/confirm-modal/ && npm publish --access public",
    "build4": "ng build --project=@tots/crud-page --configuration production && cd dist/tots/crud-page/ && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
